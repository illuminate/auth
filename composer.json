{
    "name": "illuminate/auth",
    "description": "A flexible, driver based authentication library.",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/cookie": "1.2.*",
        "illuminate/encryption": "1.2.*",
        "illuminate/hashing": "1.2.*",
        "illuminate/session": "1.2.*",
        "illuminate/support": "1.2.*",
        "symfony/http-foundation": "2.2.*"
    },
    "require-dev": {
        "illuminate/database": "1.2.*",
        "mockery/mockery": ">=0.7.2"
    },
    "autoload": {
        "psr-0": {"Illuminate\\Auth": "src/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev"
}